{
   "name":"Keyboard control for youtube",
   "description": "Cross-tab keyboard control of youtube video and playlist playback",
   "version":"1.0.6",
   "manifest_version": 2,
   "content_scripts":[
      {
         "matches":[
            "<all_urls>"
         ],
         "js":[
            "listener.js"
         ]
      }
   ],
   "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
   },
   "permissions":[
      "<all_urls>",
      "background",
      "tabs"
   ]
}
